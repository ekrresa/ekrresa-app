{
  "name": "ekrresa-app",
  "version": "1.0.0",
  "description": "My personal website",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "Ochuko Ekrresa",
  "license": "MIT",
  "private": true,
  "scripts": {
    "copy-cover-images": "node scripts/copy-cover-images.mjs",
    "build": "pnpm copy-cover-images && vite build",
    "dev": "pnpm copy-cover-images && vite dev",
    "dev:init": "rw-scripts dev-init",
    "preview": "vite preview",
    "worker:run": "rw-scripts worker-run",
    "clean": "pnpm clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "rw-scripts ensure-deploy-env && pnpm clean && pnpm build && wrangler deploy",
    "generate": "rw-scripts ensure-env && wrangler types --include-runtime false",
    "check": "pnpm generate && pnpm types",
    "types": "tsc",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format --check ."
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "core-js": "^3.47.0",
    "react": "19.2.3",
    "react-aria-components": "^1.14.0",
    "react-dom": "19.2.3",
    "react-server-dom-webpack": "19.2.3",
    "rwsdk": "1.0.0-beta.44",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@cloudflare/vite-plugin": "1.16.1",
    "@content-collections/core": "^0.13.0",
    "@content-collections/markdown": "^0.1.4",
    "@content-collections/mdx": "^0.2.2",
    "@content-collections/vite": "^0.2.8",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@mdx-js/react": "3.1.1",
    "@mdx-js/rollup": "3.1.1",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "~24.5.2",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "eslint-plugin-better-tailwindcss": "^3.8.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-react-aria-components": "^2.0.1",
    "typescript": "5.9.3",
    "vite": "7.2.6",
    "vite-plugin-svgr": "^4.5.0",
    "wrangler": "4.52.1",
    "zod": "^4.3.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js",
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
